{
  "kind": "collectionType",
  "collectionName": "subscriptions",
  "info": {
    "name": "Subscriptions",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "minLength": 2
    },
    "subdomain": {
      "type": "string",
      "required": true,
      "minLength": 3
    },
    "numberOfUsers": {
      "type": "integer",
      "required": true,
      "min": 1,
      "default": 1
    },
    "paymentRecurrence": {
      "type": "enumeration",
      "enum": [
        "monthly",
        "yearly"
      ],
      "required": true,
      "default": "monthly"
    },
    "subTotal": {
      "type": "decimal",
      "required": true,
      "min": 0.01
    },
    "total": {
      "type": "decimal",
      "required": true,
      "min": 0.01
    },
    "discount": {
      "type": "integer",
      "required": false,
      "min": 1
    },
    "active": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "services": {
      "collection": "services"
    },
    "coupon": {
      "model": "coupons"
    },
    "owner": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "partner": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "expires_at": {
      "type": "datetime",
      "required": false
    }
  }
}
